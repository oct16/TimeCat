{
    "name": "timecatjs",
    "author": "oct16",
    "version": "1.0.16",
    "description": "TimeCat Web Recorder",
    "main": "lib/timecatjs.cjs.js",
    "module": "lib/timecatjs.esm.js",
    "types": "lib/timecatjs.d.ts",
    "keywords": [
        "recorder",
        "replay",
        "player",
        "virtual-dom",
        "screenshots",
        "audio",
        "video",
        "chrome",
        "chrome-extension"
    ],
    "license": "GPL-3.0-or-later",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "dev": "node scripts/dev.js",
        "build": "node scripts/build.js",
        "live": "node scripts/live.js",
        "lib": "node scripts/lib.js",
        "test": "jest",
        "f": "npx code-formatter --path packages",
        "doc": "npx typedoc",
        "embed": "cp packages/timecat/dist/timecatjs.min.js ../TimeCatChrome/src/assets/",
        "count": "find ./packages -type f -not -path **/*.spec.ts -not -path **/global.* -not -path './node_modules/*' -not -path '**/dist/*'  -name '*.ts' | xargs wc -l"
    },
    "devDependencies": {
        "@microsoft/api-extractor": "^7.8.9",
        "@oct16/code-formatter": "^1.0.9",
        "@rollup/plugin-commonjs": "^11.0.2",
        "@rollup/plugin-html": "^0.1.1",
        "@rollup/plugin-node-resolve": "^7.1.3",
        "@rollup/plugin-replace": "^2.3.1",
        "@types/diff": "^4.0.2",
        "@types/jest": "^26.0.0",
        "@types/node": "^13.9.2",
        "@types/pako": "^1.0.1",
        "@types/socket.io-client": "^1.4.32",
        "@zerollup/ts-transform-paths": "^1.7.17",
        "diff": "^4.0.2",
        "execa": "^4.0.0",
        "fs-extra": "^8.1.0",
        "jest": "^26.0.1",
        "minimist": "^1.2.5",
        "pako": "^1.0.11",
        "rollup": "^1.32.0",
        "rollup-plugin-browsersync": "^1.1.0",
        "rollup-plugin-copy": "^3.3.0",
        "rollup-plugin-delete": "^1.2.0",
        "rollup-plugin-scss": "^2.5.0",
        "rollup-plugin-string": "^3.0.0",
        "rollup-plugin-terser": "^5.3.0",
        "rollup-plugin-typescript2": "^0.26.0",
        "rollup-plugin-visualizer": "^4.0.4",
        "ts-jest": "^26.1.0",
        "ttypescript": "^1.5.10",
        "typedoc": "^0.17.7",
        "typescript": "^3.9.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/oct16/TimeCat.git"
    },
    "bugs": {
        "url": "https://github.com/oct16/TimeCat/issues"
    },
    "homepage": "https://github.com/oct16/TimeCat#readme",
    "files": [
        "lib"
    ]
}
