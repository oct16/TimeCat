{
    "private": true,
    "name": "timecatjs",
    "author": "oct16",
    "version": "1.1.4",
    "description": "TimeCat Web Recorder",
    "main": "lib/timecat.cjs.js",
    "module": "lib/timecat.esm.js",
    "types": "lib/timecat.d.ts",
    "files": [
        "lib"
    ],
    "keywords": [
        "recorder",
        "replay",
        "player",
        "virtual-dom",
        "screenshots",
        "audio",
        "video",
        "chrome",
        "chrome-extension"
    ],
    "license": "GPL-3.0-or-later",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "dev": "node scripts/dev.js",
        "build": "node scripts/build.js",
        "live": "node scripts/live.js",
        "lib": "node scripts/lib.js",
        "serve": "serve ./examples",
        "test": "jest",
        "embed": "cp packages/timecat/dist/timecat.global.js ../TimeCatChrome/src/assets/",
        "count": "find ./packages -type f -not -path **/*.spec.ts -not -path **/global.* -not -path './node_modules/*' -not -path '**/dist/*'  -name '*.ts' | xargs wc -l",
        "lint": "eslint 'packages/**/*.{js,ts}' --quiet --fix",
        "preinstall": "node ./scripts/checkYarn.js"
    },
    "dependencies": {},
    "devDependencies": {
        "@microsoft/api-extractor": "^7.8.9",
        "@rollup/plugin-commonjs": "^11.0.2",
        "@rollup/plugin-html": "^0.1.1",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^7.1.3",
        "@rollup/plugin-replace": "^2.3.1",
        "@types/diff": "^4.0.2",
        "@types/jest": "^26.0.0",
        "@types/node": "^13.9.2",
        "@types/pako": "^1.0.1",
        "@types/smoothscroll-polyfill": "^0.3.1",
        "@typescript-eslint/eslint-plugin": "^3.9.0",
        "@typescript-eslint/parser": "^3.9.0",
        "@zerollup/ts-transform-paths": "^1.7.17",
        "brotli": "^1.3.2",
        "browser-sync": "^2.26.12",
        "diff": "^4.0.2",
        "eslint": "^7.6.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "execa": "^4.0.0",
        "fs-extra": "^8.1.0",
        "jest": "^26.0.1",
        "koa": "^2.13.0",
        "minimist": "^1.2.5",
        "pako": "^1.0.11",
        "prettier": "^2.0.5",
        "rollup": "^1.32.0",
        "rollup-plugin-scss": "^2.5.0",
        "rollup-plugin-string": "^3.0.0",
        "rollup-plugin-terser": "^5.3.0",
        "rollup-plugin-typescript2": "^0.26.0",
        "rollup-plugin-visualizer": "^4.0.4",
        "smoothscroll-polyfill": "^0.4.4",
        "ts-jest": "^26.1.0",
        "typescript": "^3.9.7"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/oct16/TimeCat.git"
    },
    "bugs": {
        "url": "https://github.com/oct16/TimeCat/issues"
    },
    "homepage": "https://github.com/oct16/TimeCat#readme"
}
